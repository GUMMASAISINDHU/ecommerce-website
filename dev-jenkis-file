pipeline {
  agent any
    stages {
       stage("check out") {
         steps{
           checkout scm
         }
       }
      stage("Build and test(Dev)"){
        steps{
          echo 'Building and testing code in Dev environment...'
        }
      }
     stage("Deploy to UAT"){
       steps{
         echo "Testing code in UAT environment"
       }
     }
    stage("Deploy to production"){
      steps{
        echo "deploying to production"
      }
    }
  }
  

pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                // Checkout the Git repository
                checkout scm
                // Print the commit ID
                echo "Building commit ID: ${env.GIT_COMMIT}"
            }
        }
        stage('Build') {
            steps {
                // Your build steps go here
                echo "Building the project..."
            }
        }
        stage('Test') {
            steps {
                // Your test steps go here
                echo "Running tests..."
            }
        }
        stage('Deploy') {
            steps {
                // Your deploy steps go here
                echo "Deploying the project..."
            }
        }
    }
}
